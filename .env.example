# For production with high traffic, use redis for better performance and persistence
CACHE_STORE=octane

# Response Cache Settings (Spatie ResponseCache)
# Cache lifetime: 1 year by default (cleared on content changes)
RESPONSE_CACHE_ENABLED=true
# RESPONSE_CACHE_LIFETIME=31536000
