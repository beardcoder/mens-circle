{
  "name": "mens-circle",
  "private": true,
  "workspaces": ["cms", "web"],
  "scripts": {
    "dev": "bun run --filter '*' dev",
    "dev:cms": "bun run --filter cms dev",
    "dev:web": "bun run --filter web dev",
    "build": "bun run --filter cms build && bun run --filter web build",
    "build:cms": "bun run --filter cms build",
    "build:web": "bun run --filter web build"
  }
}
