{
    "name": "beardcoder/mens-circle",
    "type": "typo3-cms-extension",
    "description": "Männerkreis Niederbayern - Event Management und Community Plattform für TYPO3 v14",
    "license": "MIT",
    "authors": [
        {
            "name": "beardcoder",
            "role": "Developer"
        }
    ],
    "require": {
        "beardcoder/fluid-forms": "@dev",
        "php": "^8.3 || ^8.5",
        "typo3/cms-core": "^14.1",
        "typo3/cms-extbase": "^14.1",
        "typo3/cms-fluid": "^14.1",
        "typo3/cms-frontend": "^14.1",
        "typo3/cms-form": "^14.1",
        "typo3/cms-seo": "^14.1"
    },
    "require-dev": {
        "typo3/testing-framework": "^9.0",
        "phpstan/phpstan": "^2.0",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "BeardCoder\\MensCircle\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "mens_circle"
        }
    },
    "scripts": {
        "test": "phpunit",
        "lint": "phpstan analyse",
        "format": "php-cs-fixer fix"
    }
}
