@import './TypoScript/*.typoscript'

config {
  sendCacheHeaders = 1
  compressCss = 1
  concatenateCss = 1
  minifyCss = 1
  compressJs = 1
  concatenateJs = 1
  minifyJs = 1
}

page = PAGE
page {
  typeNum = 0

  bodyTagCObject = TEXT
  bodyTagCObject {
    data = sitesettings:mensCircle.theme
    wrap = <body class="theme-- | ">
  }

  10 =< lib.fluidPage
}
