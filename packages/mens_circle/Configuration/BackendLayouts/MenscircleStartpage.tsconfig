                                }
                            }
                            3 {
                                columns {
                                    1 {
                                        name = mens_circle.backend_layouts:backend_layout.colPos.footer
                                        colPos = 10
                                        identifier = footer
                                        slideMode = slide
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
